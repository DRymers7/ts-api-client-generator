{
    "name": "ts-api-client-generator",
    "version": "1.0.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "Automatically generate React components with useQuery from API request files (HTTP, cURL, Postman JSON)",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "bin": {
        "generate-api-client": "dist/bin/cli.js"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "dev": "tsx src/cli/cli.ts",
        "build": "rimraf dist && rollup --bundleConfigAsCjs -c",
        "prepublishOnly": "npm run build",
        "format": "prettier . --write",
        "lint": "eslint . --ext .ts,.tsx --fix",
        "test": "vitest run",
        "test:ui": "vitest --ui",
        "test:watch": "vitest --watch",
        "test:debug": "vitest --run --inspect",
        "test:unit": "vitest run tests/unit",
        "test:coverage": "vitest run --coverage"
    },
    "keywords": [
        "react",
        "typescript",
        "api",
        "client-generator",
        "useQuery",
        "tanstack-query",
        "postman",
        "curl",
        "http",
        "code-generation",
        "rest-api"
    ],
    "author": "Your Name <your.email@example.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/ts-api-client-generator.git"
    },
    "homepage": "https://github.com/yourusername/ts-api-client-generator#readme",
    "bugs": {
        "url": "https://github.com/yourusername/ts-api-client-generator/issues"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "axios": "^1.9.0",
        "commander": "^14.0.0",
        "shell-quote": "^1.8.2",
        "ts-morph": "^25.0.1",
        "prettier": "^3.5.3"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/node": "^22.15.18",
        "@types/shell-quote": "^1.7.5",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.0.0",
        "rimraf": "^5.0.0",
        "rollup": "^4.0.0",
        "rollup-plugin-dts": "^6.0.0",
        "tsx": "^4.0.0",
        "typescript": "^5.8.3",
        "vitest": "^3.1.3"
    },
    "peerDependencies": {
        "@tanstack/react-query": "^4.0.0 || ^5.0.0",
        "react": "^17.0.0 || ^18.0.0"
    }
}